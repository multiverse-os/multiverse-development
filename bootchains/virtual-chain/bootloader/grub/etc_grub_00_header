	}
	menuentry 'Debian GNU/Linux, with Linux 4.19.0-8-amd64 (recovery mode)' --class debian --class gnu-linux --class gnu --class os $menuentry_id_option 'gnulinux-4.19.0-8-amd64-recovery-6d4ff40c-ccdf-4c0a-adb5-c6fe03ec7b87' {
		load_video
		insmod gzio
		if [ x$grub_platform = xxen ]; then insmod xzio; insmod lzopio; fi
		insmod part_gpt
		insmod ext2
		if [ x$feature_platform_search_hint = xy ]; then
		  search --no-floppy --fs-uuid --set=root  59e47608-a0e6-435d-b6e3-a2c082cf349a
		else
		  search --no-floppy --fs-uuid --set=root 59e47608-a0e6-435d-b6e3-a2c082cf349a
		fi
		echo	'Loading Linux 4.19.0-8-amd64 ...'
		linux	/vmlinuz-4.19.0-8-amd64 root=/dev/mapper/host--vg-root ro single 
		echo	'Loading initial ramdisk ...'
		initrd	/initrd.img-4.19.0-8-amd64
	}
	menuentry 'Debian GNU/Linux, with Linux 4.9.0-11-amd64' --class debian --class gnu-linux --class gnu --class os $menuentry_id_option 'gnulinux-4.9.0-11-amd64-advanced-6d4ff40c-ccdf-4c0a-adb5-c6fe03ec7b87' {
