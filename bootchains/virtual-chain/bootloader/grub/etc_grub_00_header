		insmod part_gpt
		insmod ext2
		if [ x$feature_platform_search_hint = xy ]; then
		  search --no-floppy --fs-uuid --set=root  59e47608-a0e6-435d-b6e3-a2c082cf349a
		else
		  search --no-floppy --fs-uuid --set=root 59e47608-a0e6-435d-b6e3-a2c082cf349a
		fi
