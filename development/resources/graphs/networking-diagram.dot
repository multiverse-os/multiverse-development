digraph MultiverseNetworking {
  subgraph cluster_0 {
    label = "Multiverse OS";
    style=filled;
    color=lightgrey;
    node [shape=Square,style=filled,color=white];
  
    subgraph cluster_1 {
      label = "Universe Router";
      style=filled;
      color=white;
      node [shape=Square,style=filled,color=lightgrey];
  
      PhysicalNIC [color=lightgrey];

    }
    
    PhysicalNIC -> virbr0;
    "Star Router" -> virbr2;
    "Star Router" -> virbr1;
    "Galaxy Router" -> virbr1;
    "Galaxy Router" -> virbr0;
    ControllerVM -> virbr1;
    ControllerVM -> virbr2;
  }

  PhysicalNIC -> LAN;
  LAN [shape=Square];
}
