KEYMAPOPTS="us us"
HOSTNAMEOPTS="-n host"
INTERFACESOPTS="auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
	address 10.6.6.2
	netmask 255.255.255.0
	gateway 10.6.6.1

auto eth1
iface eth0 inet static
	address 10.7.7.1
	netmask 255.255.255.0
	gateway 10.7.7.1
"

DNSOPTS="-d 9.9.9.9"
TIMEZONEOPTS="-z UTC"
PROXYOPTS="none"
APKREPOSOPTS="-f"
SSHDOPTS="none"
NTPOPTS="-c chrony"
DISKOPTS="-m sys /dev/vda"
